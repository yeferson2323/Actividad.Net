@model IEnumerable<RoarMot.Models.Moto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MarcaMoto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModeloMoto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ColorMoto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImagenMoto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SoatMoto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tecnomecanica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlacaMoto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kilometraje)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioIdUsuarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MarcaMoto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModeloMoto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ColorMoto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImagenMoto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoatMoto)
            </td>
