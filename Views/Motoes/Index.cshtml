            <td>
                @Html.DisplayFor(modelItem => item.PlacaMoto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Kilometraje)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioIdUsuarioNavigation.IdUsuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDatosmoto">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdDatosmoto">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdDatosmoto">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
